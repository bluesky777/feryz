	.panel-heading Antecedentes Laborales

	.panel-body(style="margin: 2px 0px 10px 5px")

		.row
			.col-lg-4.col-xs-4
				table.antecedentes(border="1px solid")
					thead
						tr
							th
								span.negrita Empresa
							th 
								span.negrita Activida economica
							th 
								span.negrita Cargo
							th(style="min-width:100px; ") 
								span.negrita Factores de riesgo
							th
								span.negrita Tiempo de exposicion
							th 
								span.negrita Nivel de exposicion
							th 
								span.negrita Controles
							th 
								span.negrita Epp
					tbody
						tr(ng-repeat="antecedente in exameningreso.ant_laborales") 
							td {{antecedente.empresa}} 
							td {{antecedente.activ_economica}} 
							td {{antecedente.cargo}} 
							td {{antecedente.fact_riesgo}} 
							td {{antecedente.tiempo_exposicion}} 
							td {{antecedente.niv_exposicion}} 
							td {{antecedente.controles}} 
							td {{antecedente.epp}}


