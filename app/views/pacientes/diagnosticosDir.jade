
.panel.panel-default.panel-hovered.mb20
	.panel-heading Diganosticos

	.panel-body
		
		.row
			.col-lg-6.col-sm-6
				md-input-container.md-block(flex-gt-sm style="margin: 0!important; padding-bottom: 0!important")
					label Diagnostico
					input(ng-model="pacienteEdit.diagnostico.diagnostico")
		
			.col-lg-6.col-sm-6.col-xs-12
				md-input-container.md-block(flex-gt-sm style="margin: 0!important; padding-bottom: 0!important")
					label Nivel de aptitud
					input(ng-model="pacienteEdit.diagnostico.nivel_aptitud")

		
		.row
			.col-lg-6.col-sm-12.col-xs-12
				md-input-container.md-block(flex-gt-sm style="margin: 0!important; padding-bottom: 0!important")
					label Restricciones / Temporales o Permanentes
					input(ng-model="pacienteEdit.diagnostico.restricciones_temp_perm" value='No presenta resticcion alguna pra el cargo que aspira')
			.col-lg-6.col-sm-12.col-xs-12
				md-input-container.md-block(flex-gt-sm style="margin: 0!important; padding-bottom: 0!important")
					label Recomendaciones de Diagnostico
					input(ng-model="pacienteEdit.diagnostico.recomendaciones_diagnostico")

						


 
		