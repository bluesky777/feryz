.panel.panel-default.panel-hovered.mb20(ng-if="hasRoleOrPerm(6)")

			.panel-heading Examen fisico
			.panel-body
				div
					form(ng-submit="guardarPaciente()")

						.row
							.col-lg-12
								h6 SIGNOS VITALES
						.row
							.col-lg-2
								md-input-container.md-block(flex-gt-sm style="margin: 0!important; padding-bottom: 0!important")
									label FR (min)
									input(ng-model="pacienteEdit.examen_fisico.sign_vit_fr")
							.col-lg-1
								md-input-container.md-block(flex-gt-sm style="margin: 0!important; padding-bottom: 0!important")
									label TA
									input(ng-model="pacienteEdit.examen_fisico.sign_vit_ta")
							.col-lg-1(style="margin-left: -20px !important;margin-right: -100px;margin-top: 9px;")
								| /
							.col-lg-1

								md-input-container.md-block(flex-gt-sm style="margin: 0!important; padding-bottom: 0!important")
									label mmHG
									input(ng-model="pacienteEdit.examen_fisico.sign_vit_ta1")
							.col-lg-1
								md-input-container.md-block(flex-gt-sm style="margin: 0!important; padding-bottom: 0!important")
									label Temp
									input(ng-model="pacienteEdit.examen_fisico.sign_vit_temp")
							.col-lg-2
								md-input-container.md-block(flex-gt-sm style="margin: 0!important; padding-bottom: 0!important")
									label FC (min)
									input(ng-model="pacienteEdit.examen_fisico.sign_vit_fc")
							.col-lg-2
								md-input-container.md-block(flex-gt-sm style="margin: 0!important; padding-bottom: 0!important")
									label PESO (kg)
									input(ng-model="pacienteEdit.examen_fisico.sign_vit_peso")
							
						
							.col-lg-2
								md-input-container.md-block(flex-gt-sm style="margin: 0!important; padding-bottom: 0!important")
									label TALLA (cm)
									input(ng-model="pacienteEdit.examen_fisico.sign_vit_talla")
						.row
							.col-lg-2
								md-input-container.md-block(flex-gt-sm style="margin: 0!important; padding-bottom: 0!important")
									label IMC (Kg/m)
									input(ng-model="pacienteEdit.examen_fisico.sign_vit_imc")

						.row
							.col-lg-12
								h6 Estado general
						.row
							.form-group.col-lg-12.col-sm-12.col-xs-12
							
								ui-select(ng-model="pacienteEdit.examen_fisico.estado_general" style="margin: 0!important;width: 100%; padding-bottom: 0!important")
									ui-select-match(placeholder="Estado General" allow-clear="true") {{$select.selected.descripcion}}
									ui-select-choices(repeat="estado in estados_generales | filter: $select.search")
										div(ng-bind-html="estado.descripcion | highlight: $select.search")


						.row
							.col-lg-6
								.row
									.col-lg-12.col-sm-12.col-xs-12
										h6 Constituci√≥n
