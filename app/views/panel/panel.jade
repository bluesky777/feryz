header.site-head.fixedHeader#site-head(ng-class="{'fixedHeader': fixedHeader}" ng-include=" 'views/panel/header.tpl.html' ")
	




// main-container
.main-container.clearfix.nav-expand(ng-class="{'nav-expand': navFull, 'nav-horizontal': navHorizontal}")

	aside.nav-container#nav-container(ng-class="{ 'nav-fixed': main.fixedSidebar, 'nav-horizontal': main.menu === 'horizontal', 'nav-vertical': main.menu === 'vertical', 'bg-white': ['31','32','33','34','35','36'].indexOf(main.skin) >= 0, 'bg-dark': ['31','32','33','34','35','36'].indexOf(main.skin) < 0 }" ng-include=" 'views/panel/nav.tpl.html' ")
		.nav-head

	div.content-container.fixedHeader(ng-class="{'nav-expand': navFull, 'fixedHeader': fixedHeader}" id="content")
		.page.page-dashboard

			.page-wrap(ui-view="contenido_panel")

				iniciar-dir(eventoactual="evento_actual" user="USER")



	<!-- settings -->
	.site-settings.clearfix.hidden-xs(ng-class="{'open': isSettingsOpen}")
		.settings.clearfix
			.trigger.ion.ion-settings.left(ng-click="toggleSettingsBox()" role="button" tabindex="0")
			.wrapper.left
				ul.list-unstyled.other-settings
					li.clearfix.mb10
						.left.small Nav Horizontal
						md-switch.right.md-default-theme(aria-label="Mensajito" ng-model="navHorizontal" style="margin: 0" ng-change="onNavHorizontal()" role="checkbox")
							.md-container
								.md-bar
								.md-thumb-container 
								.md-thumb(md-ink-ripple="" md-ink-ripple-checkbox="")
							.md-label
				hr
				ul.themes.list-unstyled
					li(ng-class="{active: theme-zero}" ng-model="themeModel" btn-radio="'theme-zero'" ng-change="onThemeChange(themeModel)" tabindex="0" aria-invalid="false")


